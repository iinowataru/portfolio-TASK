 <div class="container-fluid col-lg-10">
 <div class="row">
  <div class="col-md-3 mb-3">
  </div>
 </div>
  <h1>会員一覧</h1>
  <table class = 'table'>
  <% @customers.each do |customer| %>
   <tr>
     <td></td><%= attachment_image_tag customer, :image, :fill, 100, 100, format: 'jpeg' %></td>
     <th>名前</th><th></th>
     <td><%= customer.last_name %></td>
     <td><%= customer.first_name %></td>
     <td><%= link_to "詳細ページ", customer_path(customer), class:'btn btn-success' %></td>
   </tr>
  <% end %>
  </table>
</div>